<table style="width:100%" class="table table-bordered table-condensed table-hover">
	<tr>
		<strong>
			<th>User id</th>
			<th>User Name</th>
			<th>Email as stored</th>
			<th>Email decrypted</th>
			<th>Password hash</th>
		</strong>
	</tr>
	{{#each outputLines}}
	<tr>
		<td>
			<h5>{{this.id}}</h5>
		</td>
		<td>
			<h5>{{this.name}}</h5>
		</td>
		<td>
			<h5>{{this.email_hash}}</h5>
		</td>
		<td>
			<h5>{{this.email}}</h5>
		</td>
		<td>
			<h5>{{this.password_hash}}</h5>
		</td>
	</tr>
	{{/each}}
</table>



